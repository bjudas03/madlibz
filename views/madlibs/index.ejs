<h1>Madlibz</h1>

<% madlibs.forEach(function(madlib) { %>
  <div class="well">
    <h2>
      <a href="/madlibs/<%= madlib.id %>"><%= madlib.title %></a>
    </h2>
    <a href="/madlibs/<%= madlib.id %>/edit" class="btn btn-info glyphicon glyphicon-edit"></a>
  	<a href="/madlibs/<%= madlib.id %>" class="btn btn-danger glyphicon glyphicon-trash delete-btn"></a>
  </div>
<% }); %>

<hr />

<a href="/profile">&larr; Back Home</a>
